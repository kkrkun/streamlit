{
    "app_id": "", // SkyWay Application ID// SkyWayのアプリケーションID
    "secret_key": "", // SkyWay Secret Key// SkyWayのシークレットキー

    "username": "", // TCP Exposer Username// TCP Exposerのユーザー名
    "sub_domain": "", // TCP Exposer Subdomain (Used for RoomID)// RoomIDに使用するTCP Exposerのサブドメイン
    "sub_domain2": "", // TCP Exposer Subdomain (Used for Minecraft Connection, usually left blank)// minecraftとの連携に使用するTCP Exposerのサブドメイン（通常は空欄）
    "ssh_password": "", // TCP Exposer Password// TCP Exposerのパスワード

    "port": 19132, // Port number used for Minecraft connection// minecraftとの連携に使用するポート番号
    "web_port": 8000, // Port number used for Website connection// Webサイトとの連携に使用するポート番号

    "proximity": true, // Enable Proximity VC (default: true)// 近接VCを有効にするか (デフォルト: true)
    "spectator": true, // Separate VC for players in Spectator mode (default: true)// スペクテイターモードのプレイヤーはVCを分けるか (デフォルト: true)

    // Settings effective only when "spectator" is true// "spectator"がtrueの場合のみ有効な設定
    "specListen": true, // Allow spectators to hear non-spectator players (default: true)// スペクテイターが他モードプレイヤーの会話を聞けるようにするか (デフォルト: true)
    "specDim": false, // Separate conversations among spectators by dimension (default: false)// スペクテイター同士の会話をディメンションごとに分けるか (デフォルト: false)
    "password": false, // Require password for Proximity VC connection (default: false)// 近接VC接続時にパスワードを要求するか (デフォルト: false)
    
    "distance": 10, // Maximum block distance for voice to reach in-game (default: 10.0)// ゲーム内で声が届く最大ブロック数 (デフォルト: 10.0)
    "lang": "en" // Language setting ("ja" or "en")// 表示言語の設定 ("ja" または "en")
}
